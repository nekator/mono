{
  "name": "patternhub",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm-run-all compile build:*",
    "build:app": "next build",
    "build:copy": "cpy out/** ../../build-showcases/patternhub",
    "compile": "npm-run-all compile:*",
    "compile:01_jsx": "node scripts/generate-example-jsx.js",
    "compile:02_copy-docs": "cpy ../../docs ./public",
    "compile:03_components": "node scripts/esbuild-generate.mjs",
    "compile:04_run": "node generated.js",
    "dev": "cross-env NEXT_PUBLIC_BASE_PATH=/mono/sub NODE_OPTIONS='--inspect' npm-run-all compile:* -p open next:dev",
    "lint": "next lint",
    "next:dev": "next dev",
    "open": "open-cli http://localhost:3000/mono/sub",
    "prebuild:app": "cpy ../../node_modules/iframe-resizer/js/** public/iframe-resizer",
    "start": "cross-env NEXT_PUBLIC_BASE_PATH=/mono/sub npm-run-all compile next:dev"
  },
  "dependencies": {
    "dompurify": "3.1.5",
    "highlight.js": "^11.9.0",
    "next": "14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rehype-slug": "6.0.0",
    "remark-gfm": "4.0.0"
  },
  "devDependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.4",
    "@types/dompurify": "3.0.5",
    "@types/node": "20.14.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "esbuild": "0.21.5",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "iframe-resizer": "^4.4.2",
    "open-cli": "^8.0.0",
    "sass": "^1.77.6",
    "typescript": "5.4.5"
  }
}
